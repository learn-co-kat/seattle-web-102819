<form action="/google_books/<%= "#{@google_book.id}" %>" method="post">
    <input type="hidden", name="_method", value="patch">
    <label for="title">
        <input type="text" name="title" value="<% "#{@google_book.title}"%>">
    </label>
    <label for="author">
        <input type="text" name="author" value="<% "#{@google_book.author}"%>">
    </label>
    <label for="snippet">
        <input type="text" name="snippet" value="<% "#{@google_book.snippet}"%>">
    </label>
    <input type="submit">
</form>